{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@models/*": [ "app/Models/*" ],
            "@controllers/*": [ "app/Controllers/*" ],
            "@routes/*": [ "routes/*" ],
            "@utils/*": [ "shared/*" ]
        },
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist"
    },
    "lib": ["es2015"]
}
